@{
    ViewData["Title"] = "Kategória";
}
<link rel="stylesheet" href="~/css/order.css" />
<p>
    <div class="text-center">
        <h1>@ViewData["Title"]</h1>
    </div>
</p>
<table style="width:100%" aria-hidden="true">
        <tr>
            <td>
            <div class="container">
                <p class="spaceing">
                    <a asp-action="ServiceSelect" asp-route-category="@Convert.ToInt32(NeedsEnums.CategoryType.Car)">
                        <img src="~/css/image/auto2.JPG" alt="Személyautó" style="width:350px;height:200px; border-radius: 16px;border: 2px solid black" />
                        <div class="overlayj">
                            <div class="text">Személyautó</div>
                        </div>
                    </a>
                </p>
            </div>
            </td>
            <td>
            <div class="container">
                <p class="spaceing">
                    <a asp-action="ServiceSelect" asp-route-category="@Convert.ToInt32(NeedsEnums.CategoryType.Van)">
